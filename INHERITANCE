import java.util.Scanner;
class Animal{
void eat(){
    System.out.println("eating");}
void sleep(){
    System.out.println("sleeping");}
}
class Bird extends Animal{
    void fly(){
        System.out.println("flying");}   
}
    public class inheritance{
        public static void main(String args[]){
        Bird b=new Bird();
        b.eat();
        b.sleep();
        b.fly(); }
}
